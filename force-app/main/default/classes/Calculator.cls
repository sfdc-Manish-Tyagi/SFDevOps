public with sharing class Calculator {
    public Calculator() {

    }
    public integer add(integer a, Integer b){
        return a+b;
    }
    public integer sub(integer a, Integer b){
        return a-b;
    }
    public integer mul(integer a, Integer b){
        return a*b;
    }
    public integer div(integer a, Integer b){
        return a/b;
    }
}
